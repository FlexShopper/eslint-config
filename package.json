    "eslint": "4.18.2",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-promise": "3.6.0"
  },
  "devDependencies": {
    "fixpack": "^2.3.1",
    "pre-commit": "^1.1.3"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "homepage": "https://github.com/flexshopper/eslint-config-flexshopper#readme",
  "keywords": [
