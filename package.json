{
  "name": "@flexshopper/eslint-config-flexshopper",
  "description": "ESLint FlexShopper configuration",
  "version": "6.1.0",
  "author": "Italo A. Casas <italo.casas@flexshopper.com>",
  "bugs": {
    "url": "https://github.com/flexshopper/eslint-config-flexshopper/issues"
  },
  "dependencies": {
